{
  "name": "incent-apply",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n frontend,backend \"npm run dev:frontend\" \"npm run backend:dev\"",
    "dev:frontend": "vite --host 127.0.0.1",
    "dev:all": "npm run dev",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "backend:dev": "node backend/server.js",
    "backend:dev:watch": "node --watch backend/server.js",
    "backend:start": "node backend/server.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@node-rs/argon2": "^2.0.2",
    "@tanstack/react-query": "^5.66.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "pg-mem": "^3.0.5",
    "pg": "^8.13.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/node": "^22.10.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.5"
  }
}
